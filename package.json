    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "electron": "^29.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": ">=6",
    "jsdom": "^24.0.0",
    "lint-staged": ">=10",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "~5.3.0",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.0.11",
