    "element-plus": "^2.5.6",
    "js-cookie": "^3.0.5",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "moment-business-days": "^1.2.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vue": "^3.4.15",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.10.1",
    "vue-router": "^4.2.5",
    "xlsx": "^0.18.5"
